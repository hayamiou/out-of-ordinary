<%= simple_form_for [@offer, @booking] do |f| %>

  <div class="form-inputs">
    <%= f.input :number_of_persons %>
    <%= f.input :date_start %>
    <%= f.input :date_end %>
    <%= f.input :total_prices %>
  </div>

  <div class="form-actions btn btn-primary">
    <%= f.button :submit %>
  </div>
<% end %>
